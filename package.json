{
  "name": "whatson",
  "description": "What's on?",
  "version": "1.0.0",
  "private": false,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "context-storage": "^0.0.0",
    "griding": "^0.1.2",
    "minireset.css": "^0.0.4",
    "prettier": "^2.8.3",
    "query-string": "^8.1.0",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-hooks-fetch": "^0.5.1",
    "react-intersection-observer": "^8.0.5",
    "react-lazy-progressive-image": "^1.4.0",
    "react-scripts": "3.0.1",
    "styled-components": "^4.1.3",
    "stylis": "^3.5.4",
    "typeface-roboto": "^0.0.54"
  },
  "scripts": {
    "dev": "rescripts start",
    "build": "npm run prettier && rescripts build",
    "postbuild": "react-snap && cp build/index.html build/404.html && cp build/index.html build/200.html",
    "eject": "rescripts eject",
    "prettier": "prettier --write .",
    "surge": "surge build whatson-dev.surge.sh",
    "update": "rm -rf node_modules package-lock.json && npm i --legacy-peer-deps"
  },
  "prettier": {
    "printWidth": 1000
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "rescripts": [
    "env"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@rescripts/cli": "^0.0.10",
    "@rescripts/rescript-env": "^0.0.5",
    "@vitordino/react-snap": "^1.24.0-alpha.0",
    "babel-plugin-preval": "^3.0.1"
  },
  "reactSnap": {
    "exclude": [
      "/movies/*",
      "/search"
    ]
  }
}
